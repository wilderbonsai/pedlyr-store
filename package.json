  "repository": {
    "type": "git",
    "url": "git+https://github.com/parmsang/gatsby-starter-ecommerce.git"
  },
  "scripts": {
    "dev": "rm -rf .cache gatsby develop",
    "lint": "eslint **/*.{js,jsx} --quiet",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "develop": "run-p develop:**",
    "develop:app": "rm -rf .cache && gatsby develop",
    "develop:lambda": "netlify-lambda serve src/functions",
    "build": "run-p build:**",
    "build:app": "gatsby build",
    "build:lambda": "netlify-lambda build src/functions",
    "deploy": "rm -rf .cache && gatsby build --prefix-paths && gh-pages -d public",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix gatsby-node.js"
  }
